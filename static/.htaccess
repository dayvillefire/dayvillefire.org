<IfModule mod_rewrite.c>
# Redirect all insecure requests
RewriteCond %{HTTPS} !=on
RewriteRule ^/?(.*) https://%{SERVER_NAME}/$1 [R=301,L]
</IfModule>

Header set Content-Security-Policy "default-src 'self' *.cognitoforms.com *.dayvillefire.org; font-src 'self' fonts.gstatic.com; style-src 'self' 'unsafe-inline' *.cognitoforms.com fonts.googleapis.com; script-src 'self' 'unsafe-inline' *.cognitoforms.com dayvillefire.org *.dayvillefire.org"

